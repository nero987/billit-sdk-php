{
    "name": "zodi-innovations/billit-php-sdk",
    "description": "This is an PHP SDK for the Billit API",
    "keywords": [
        "php",
        "sdk",
        "api"
    ],
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": { "Swagger\\Client\\" : "lib/" }
    },
    "autoload-dev": {
        "psr-4": { "Swagger\\Client\\" : "test/" }
    },
    "authors": [
        {
            "name": "Jens Polspoel",
            "email": "jens@zodi-innovations.be"
        }
    ],
    "require": {
        "php": ">=8.1",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "guzzlehttp/guzzle": ">=6.2"
    }
}
